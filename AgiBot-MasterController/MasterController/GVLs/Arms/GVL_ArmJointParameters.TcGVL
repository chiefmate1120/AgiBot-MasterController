VAR_GLOBAL CONSTANT
	// encoder resolution
	g_incEncoderRes :ARRAY[1..7] OF DINT :=[7(65536)];
	g_absEncoderRes :ARRAY[1..7] OF DINT :=[7(16777216)];
	
