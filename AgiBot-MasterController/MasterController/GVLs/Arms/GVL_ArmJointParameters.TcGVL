	g_maxJntPos: ARRAY[1..2,1..7] OF LREAL:=[60*g_deg2Rad,	60*g_deg2Rad-pi/2,		60*g_deg2Rad,		-pi/2,			135*g_deg2Rad+pi,		45*g_deg2Rad-pi/2,		330*g_deg2Rad,
								             60*g_deg2Rad,	60*g_deg2Rad-pi/2,		60*g_deg2Rad,		-pi/2+225*g_deg2Rad,			135*g_deg2Rad+pi,		45*g_deg2Rad+pi/2,		330*g_deg2Rad];
	
